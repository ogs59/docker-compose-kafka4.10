{
  "annotations": {
    "list": []
  },
  "description": "Kafka 4.1 Cluster Monitoring Dashboard",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Broker Overview",
      "type": "row",
      "collapsed": false
    },
    {
      "type": "graph",
      "title": "Messages In Per Sec",
      "targets": [
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_messagesin_total[1m])) by (broker)",
          "legendFormat": "{{broker}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Bytes In/Out Per Sec",
      "targets": [
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_bytesin_total[1m])) by (broker)",
          "legendFormat": "Bytes In {{broker}}"
        },
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_bytesout_total[1m])) by (broker)",
          "legendFormat": "Bytes Out {{broker}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Under Replicated Partitions",
      "targets": [
        {
          "expr": "sum(kafka_server_replicamanager_underreplicatedpartitions) by (broker)",
          "legendFormat": "{{broker}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Offline Partitions Count",
      "targets": [
        {
          "expr": "sum(kafka_controller_kafkacontroller_offlinepartitionscount) by (broker)",
          "legendFormat": "{{broker}}"
        }
      ]
    },
    {
      "title": "Topic Metrics",
      "type": "row",
      "collapsed": false
    },
    {
      "type": "graph",
      "title": "Topic Messages In Per Sec",
      "targets": [
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_messagesin_total[1m])) by (topic)",
          "legendFormat": "{{topic}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Topic Bytes In/Out Per Sec",
      "targets": [
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_bytesin_total[1m])) by (topic)",
          "legendFormat": "Bytes In {{topic}}"
        },
        {
          "expr": "sum(rate(kafka_server_brokertopicmetrics_bytesout_total[1m])) by (topic)",
          "legendFormat": "Bytes Out {{topic}}"
        }
      ]
    },
    {
      "title": "Producer Metrics",
      "type": "row",
      "collapsed": false
    },
    {
      "type": "graph",
      "title": "Producer Send Rate",
      "targets": [
        {
          "expr": "sum(rate(kafka_producer_producer_metrics_record_send_rate[1m])) by (client_id)",
          "legendFormat": "{{client_id}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Producer Error Rate",
      "targets": [
        {
          "expr": "sum(rate(kafka_producer_producer_metrics_record_error_rate[1m])) by (client_id)",
          "legendFormat": "{{client_id}}"
        }
      ]
    },
    {
      "title": "Consumer Metrics",
      "type": "row",
      "collapsed": false
    },
    {
      "type": "graph",
      "title": "Consumer Lag Max",
      "targets": [
        {
          "expr": "max(kafka_consumer_consumer_fetch_manager_metrics_records_lag_max) by (group, topic, partition)",
          "legendFormat": "{{group}}-{{topic}}-{{partition}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Consumer Records Consumed Rate",
      "targets": [
        {
          "expr": "sum(rate(kafka_consumer_consumer_fetch_manager_metrics_records_consumed_rate[1m])) by (group, topic)",
          "legendFormat": "{{group}}-{{topic}}"
        }
      ]
    },
    {
      "title": "KRaft Controller Metrics",
      "type": "row",
      "collapsed": false
    },
    {
      "type": "graph",
      "title": "Active Controller Count",
      "targets": [
        {
          "expr": "kafka_controller_kafkacontroller_activecontrollercount",
          "legendFormat": "Controller Count"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Leader Changes Per Sec",
      "targets": [
        {
          "expr": "rate(kafka_controller_kafkacontroller_leaderchangelogtotal[1m])",
          "legendFormat": "Leader Changes"
        }
      ]
    }
  ],
  "schemaVersion": 36,
  "version": 1,
  "title": "Kafka 4.1 Cluster Dashboard",
  "uid": "kafka-4-1-dashboard"
}
